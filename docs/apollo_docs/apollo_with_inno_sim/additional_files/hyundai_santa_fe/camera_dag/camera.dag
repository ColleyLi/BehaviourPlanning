module_config {
    module_library : "/apollo/bazel-bin/modules/drivers/camera/accelerated/libcompress_component_accelerated.so"

    components {
      class_name : "CompressComponentAccelerated"
      config {
        name : "camera_baumer_front_6mm_compress_accelerated"
        config_file_path : "/apollo/modules/drivers/camera/conf/camera_front_6mm.pb.txt"
        readers {
          channel: "/apollo/sensor/camera/front_6mm/image"
          pending_queue_size: 10
        }
      }
    }
}